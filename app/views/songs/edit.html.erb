<h1>Update Song</h1>
<%= form_for @song do |f| %>
    <label>Name:</label><br>
    <%= f.text_field :name %><br>
  
    <label>Artist:</label><br>
    <%= f.text_area :artist_id %><br>

    <label>Genre ID</label><br>
    <%= f.text_area :genre_id %><br>
  
    <%= f.submit "Update Song" %>
<% end %>